                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:background="#220404"
                    android:layout_marginTop="5dp"
                    />
            </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
